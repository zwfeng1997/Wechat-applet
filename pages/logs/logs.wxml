<!--logs.wxml 开锁成功页面-->
<view class="container">
  <view class="password-title">
    <text>开锁密码</text>
  </view>
  <view class="password-content">
    <text>{{password}}</text>
  </view>
  <view class="tips">
    <text>
      请使用密码解锁，{{time}}秒后开始计费
    </text>
    <view class="tips-action">
      车辆有问题？
      <text class="tips-href">
        回首页车辆报障
      </text>
    </view>
  </view>
</view>
